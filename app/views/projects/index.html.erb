<h1>Projects</h1>
<%= render partial: "components/search", locals: { resource_path: (projects_path), resource_name: "projects" } %>
<% if @error %>
  <%= render partial: "components/error", locals: { error: @error } %>
<% end %>
<% unless @resources.nil? %>
  <%= render partial: "layouts/resources", locals: { resources: map_projects(@resources, @favorites), title: nil, search_result: true} %>
<% end %>
